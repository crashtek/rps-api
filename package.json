{
  "name": "rps-api",
  "version": "1.0.0",
  "description": "RPS API",
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern webpack --ignore-pattern tests .",
    "test:setup": "./tests/harness/genCerts.sh",
    "test": "snyk test && npm run lint:js && cross-env NODE_ENV=test nyc mocha --require ignore-styles tests/mocha.js './tests/**/*.tests.js'",
    "snyk-protect": "snyk protect",
    "snyk-wizard": "snyk wizard",
    "start": "npm run lint:js && node ./index"
  },
  "author": "mostekcm",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "amqplib": "^0.5.3",
    "body-parser": "^1.18.3",
    "celebrate": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.4.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.7",
    "morgan": "^1.9.1",
    "uuid": "latest",
    "winston": "^3.2.1"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "snyk": true,
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cross-env": "^5.2.0",
    "eslint-config-auth0": "^11.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "ignore-styles": "^5.0.1",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "node-jose": "^1.1.3",
    "nyc": "^13.3.0",
    "pem-jwk": "^2.0.0",
    "sinon": "^7.3.1",
    "snyk": "^1.145.0"
  },
  "engines": {
    "node": "10.x"
  }
}
